{
    "builds": [
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/", "dest": "public/index.html" },
      { "src": "/css/(.*)", "dest": "public/css/$1" },
      { "src": "/js/(.*)", "dest": "public/js/$1" },
      { "src": "/lift.html", "dest": "public/lift.html" }
    ]
  }