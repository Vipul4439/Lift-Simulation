{
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/main.css$",
      "dest": "/main.css"
    },
    {
      "src": "^/main.js$",
      "dest": "/main.js"
    },
    {
      "src": "^/lift.html$",
      "dest": "/lift.html"
    }
  ]
}
